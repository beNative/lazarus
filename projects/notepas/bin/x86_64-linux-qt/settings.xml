			<SynHighlighter>(TSynUNIXShellScriptSyn)
				<DefaultFilter>UNIX Shell Scripts (*.sh)|*.sh</DefaultFilter>
				<Enabled>false</Enabled>
				<CommentAttri>(TSynHighlighterAttributes)
					<Foreground>clMedGray</Foreground>
					<FrameEdges>sfeAround</FrameEdges>
					<Style>[,fsItalic]</Style>
				</CommentAttri>
				<IdentifierAttri>(TSynHighlighterAttributes)
					<Foreground>clBlack</Foreground>
					<FrameEdges>sfeAround</FrameEdges>
				</IdentifierAttri>
				<KeyAttri>(TSynHighlighterAttributes)
					<Foreground>clBlue</Foreground>
					<FrameEdges>sfeAround</FrameEdges>
				</KeyAttri>
				<SecondKeyAttri>(TSynHighlighterAttributes)
					<Foreground>clBlue</Foreground>
					<FrameEdges>sfeAround</FrameEdges>
					<Style>[fsBold]</Style>
				</SecondKeyAttri>
				<SecondKeyWords>(TStringList)
					<DefinedProperties></DefinedProperties>
				</SecondKeyWords>
				<NumberAttri>(TSynHighlighterAttributes)
					<Foreground>clRed</Foreground>
					<FrameEdges>sfeAround</FrameEdges>
					<Style>[fsBold,fsItalic]</Style>
				</NumberAttri>
				<SpaceAttri>(TSynHighlighterAttributes)
					<FrameEdges>sfeAround</FrameEdges>
				</SpaceAttri>
				<StringAttri>(TSynHighlighterAttributes)
					<Foreground>clGreen</Foreground>
					<FrameEdges>sfeAround</FrameEdges>
					<Style>[fsBold,fsItalic]</Style>
				</StringAttri>
				<SymbolAttri>(TSynHighlighterAttributes)
					<FrameEdges>sfeAround</FrameEdges>
					<Style>[fsBold]</Style>
				</SymbolAttri>
				<VarAttri>(TSynHighlighterAttributes)
					<Foreground>clMaroon</Foreground>
					<FrameEdges>sfeAround</FrameEdges>
					<Style>[fsBold]</Style>
				</VarAttri>
			</SynHighlighter>
			<SmartSelectionTags>(TCodeTags)</SmartSelectionTags>
		</THighlighterItem>
	</Highlighters>
</TEditorSettings>
